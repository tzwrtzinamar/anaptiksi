#include <iostream>
#include <fstream>
#include <vector>
#include <string>
#include <cstring>
#include <sstream>
using namespace std;

class point{
	private:
		string id;
		vector<int> coordinates;
	public:
		void setID(string p_id);
		string getID();
		void setCoordinates(vector<int> p);
		vector<int> getCoordinates();
};
	
void point::setID(string p_id){
	id = p_id;
}

string point::getID(){
	return id;
}

void point::setCoordinates(vector<int> p){
	coordinates.assign(p.begin(),p.end());
}

vector<int> point::getCoordinates(){
	return coordinates;
}


int main(){
	ifstream myfile;
	string line;
	int counter=0;
	vector<int> p;
	vector<point> v;
	myfile.open("output.txt");
	while(getline(myfile,line,'\n')){
		stringstream ss ( line );
		point mypoint;
		getline(ss,line,' ');
		mypoint.setID(line);
		while(getline(ss,line,' ')){
			p.push_back(atoi(line.c_str()));
		}
		mypoint.setCoordinates(p);
		p.clear();
		v.push_back(mypoint);
	}
	myfile.close();


	return 0;
}
